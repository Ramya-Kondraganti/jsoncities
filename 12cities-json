function getTemp(){
  try{
    
   var dat="sidney"
  switch(dat){
    case "sidney": my(dat)
                   Logger.log(dat)
                   break
                   
    case "perth": my(dat)
                  Logger.log("perth")
                  break
    
    case "Hobart": my(dat)
                   Logger.log("Hobart")
                   break
            
            
    case "Toronto": my(dat)
                   Logger.log("Toronto")
                   break
                   
    case "kakinada": my(dat)
                   Logger.log("kakinada")
                   break
            
    case "Melbourne" : my(dat)
                       Logger.log("Melbourne")
                       break
            
    case "Kolkata": my(dat)
                    Logger.log("Kolkata")
                    break
    case "russia":my(dat)
                  Logger.log("russia")
                  break
    case "pune":my(dat)
                  Logger.log("pune")
                  break
  }
  
  }
catch (error){
Logger.log(error)
}

}
function my(getData) {
  var data =   {"sidney":30, "Perth":40, "
Toronto":7, "Melbourne":32, "Kolkata":35, "kakinada":34,  "pune":34, "russia":20, "hyderabad":25}
  data=JSON.stringify(data)
  var parsedData = JSON.parse(data);

  Logger.log(parsedData[getData])
}
